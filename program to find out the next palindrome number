#include<stdio.h>
#include<math.h>
int main()
{
    long n,t,r=0,c,sum=0,d;
    scanf("%ld",&n);
    while(1)
    {
        n++;
        t=n;
        while(t)
        {
            
        }
        if(r==n)
        {
            d=(int)sqrt(n);
            for(c=2;c<=d;c++)
            {
                if(n%c==0)
                break;
            }
            r=0;
        }
        printf("%ld\n",n);
         
         return 0;
    }
}
